{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "lib": ["ES2015", "DOM"],
    "downlevelIteration": true,
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}