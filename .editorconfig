root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.{json,yml,yaml,md}]
indent_size = 2

[*.{ts,js,tsx,jsx}]
indent_size = 2

[*.{html,css,scss,less}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false
